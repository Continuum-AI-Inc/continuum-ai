{
  "plugins": [
    "@nxtensions/astro"
  ],
  "npmScope": ".",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": []
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "check": {
      "inputs": [
        "production",
        "^production"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "check"
        ],
        "accessToken": "N2U5Mzc4NWItYmYwYy00ZWMxLWJjMDQtM2QzMmNhZTA3MmY2fHJlYWQtd3JpdGU="
      }
    }
  },
  "defaultProject": "main"
}
